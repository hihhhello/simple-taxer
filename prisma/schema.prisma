generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Transaction {
  id Int @id @default(autoincrement())
  amount Int
  bankName String?
  sourceName String?
  date DateTime
}
